<h3>Account Form</h3>

<%= form_for(@account) do |f| %>
    <label>Company Name: </label><br>
    <%= f.text_field :company_name %><br>

    <label>Phone Number: </label><br>
    <%= f.text_field :phone %><br>

    <label>Notes: </label><br>
    <%= f.text_area :note %><br>

    <%= f.submit %>
<% end %>